{
  "name": "Link To Phone",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Conveniently send links from your browser to any valid US cellphone number.",
  "permissions": [
    "https://www.link-to-phone.com/",
    "background",
    "storage",
    "activeTab"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": {},
    "default_popup": "popup.html",
    "default_title": "Link-To-Phone",
    "options_page": "options.html"
  },
  "icons": { "128": "icon.png" }
}
